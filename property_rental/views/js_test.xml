<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <!--    Form View to edit data in a form-->
    <record id="js_test_form" model="ir.ui.view">
        <field name="name">js.test.form</field>
        <field name="model">property.details</field>
        <field name="arch" type="xml">
            <form string="JS_TEST_FORM" editable="bottom">
                <sheet>

                </sheet>
            </form>
        </field>
