        <field name="res_model">property.details</field>
        <field name="view_mode">form</field>
        <field name="view_ids" eval="[(5, 0, 0),
                        (0, 0, {'sequence':5, 'view_mode': 'form', 'view_id': ref('property_rental.js_test_form')})]"/>
    </record>
</odoo>